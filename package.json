{
  "name": "express-sequelize-typescript-template",
  "version": "1.0.0",
  "description": "Express REST API template with Sequelize and TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "test": "jest --coverage --runInBand ./tests",
    "build": "rimraf dist && tsc -p tsconfig.build.json && resolve-tspaths -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint ./src/**/*.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "http-status": "^1.7.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.9.7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.5.9",
    "@types/oracledb": "^5.3.1",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "resolve-tspaths": "^0.8.18",
    "rimraf": "^5.0.1",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
